{
    "name": "textualization/sentence-transphormers",
    "description": "Compute Sentence Transformer embeddings using ONNX framework.",
    "type": "library",
    "support": {
        "issues": "https://github.com/textualization/sentence-transphormers/issues",
        "source": "https://github.com/textualization/sentence-transphormers"
    },
    "funding": [
        {
            "type": "ko-fi",
            "url": "https://ko-fi.com/textualization"
        }
    ],
    "require": {
        "ankane/onnxruntime": "v0.1.7",
        "textualization/ropherta": "v0.0.7"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Textualization\\SentenceTransphormers\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Pablo Duboue",
            "email": "pablo.duboue@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload-dev": {
        "psr-4": {
            "Textualization\\SentenceTransphormers\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit -d memory_limit=-1 tests"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.8"
    }
}
